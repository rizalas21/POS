generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  userid String @id @default(uuid())
  email String @unique
  name String
  password String
  role String
}

model Units {
  unit String @id @db.VarChar(10)
  name String @db.VarChar(100)
  note String?
}

